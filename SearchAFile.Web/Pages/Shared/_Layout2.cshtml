                        <form asp-controller="Account" asp-action="LogOut" method="post" id="logout_form"></form>
                    </div>
                }
                else
                {
                    <div class="d-flex align-items-center cus-bg-primary" style="border-bottom: 3px solid var(--secondary-color); height: 4rem;">
