@using Microsoft.AspNetCore.Http;
@using SearchAFile.Core.Domain.Entities;
@{
    SystemInfo? SystemInfo = Context.Session.GetObject<SystemInfo>("SystemInfo");

    Company? Company = Context.Session.GetObject<Company>("Company");
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="icon" href="~/SystemFiles/@SystemInfo.Favicon">
    <title>@Context.Session.GetString("PageTitle") | @SystemInfo.SystemName</title>

    @* Bootstrap v5.3.2 CSS *@
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">

    @* jQuery autocomplete box. *@
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    @* Font Awesome Free v6.x *@
    <script src="https://kit.fontawesome.com/9716da10c3.js" crossorigin="anonymous" asp-append-version="true"></script>

    @* Project CSS ' *@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

    @* Custom buttons *@
    <link href="~/css/cus-buttons.css" rel="stylesheet" />

    @* Project Bootstrap extensions *@
    <link rel="stylesheet" href="~/css/bootstrap-extensions.css" asp-append-version="true" />

    <style>
        :root {
            --primary-color: @(Company != null ? Company.PrimaryColor : SystemInfo.PrimaryColor); /* This is the main color. */
            --secondary-color: @(Company != null ? Company.SecondaryColor : SystemInfo.SecondaryColor); /* This is the accent color. */
            --primary-text-color: @(Company != null ? Company.PrimaryTextColor : SystemInfo.PrimaryTextColor); /* This is the color of text against a white, light gray, etc. background. */
            --secondary-text-color: @(Company != null ? Company.SecondaryTextColor : SystemInfo.SecondaryTextColor); /* This is the color of text against a colored background. */
        }
    </style>
</head>

<body id="body" style="display: flex; flex-flow: column; min-height: 100vh; opacity: 0;">

    <div id="divLoading" class="cus-progress-overlay" style="display: none;">
        <div class="cus-modal-progress">
            <div class="cus-progress">
                <img alt="Loading..." src="~/SystemFiles/loading.gif" height="100" width="100" />
            </div>
        </div>
    </div>

    <img id="imgLoading" alt="Loading..." src="~/SystemFiles/loading.gif" height="50" width="50" style="display: none;" />

    <header id="header" class="sticky-top cus-bg-primary d-flex align-items-center px-3 py-2 cus-no-select" style="height: 4rem;">
        <i class="fa-solid fa-bars fa-2x cus-text-secondary-text" data-bs-toggle="modal" data-bs-target="#divMenu" style="cursor: pointer;"></i>
        <span class="fs-4 px-3 cus-text-secondary-text-no-hover" style="line-height: 1.5rem;">@Context.Session.GetString("PageTitle")</span>
        <img class="ms-auto me-3" src="@Context.Session.GetString("Logo")" style="height: 45px;" />
    </header>

    <div id="divMenu" class="modal fade left p-0 cus-no-select" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-slideout modal-dialog-slideout-left modal-sm cus-bg-primary" role="document">
            <div id="divMenuContent" class="modal-content cus-bg-primary" style="border-radius: 0; overflow: hidden;">

                @if (User != null
                    && User.Identity.IsAuthenticated
                    && Context.Session.GetObject<UserDto>("User") == null)
                {
                    TempData["StartupJavaScript"] = "alert('Your session has expired. You have been logged out.'); $('#logout_form').submit();";
                    <form id="logout_form" asp-controller="Account" asp-action="LogOut" method="post"></form>
                }
                else if (User != null
                && User.Identity.IsAuthenticated
                && Context.Session.GetObject<UserDto>("User") != null)
                {
                    UserDto User = Context.Session.GetObject<UserDto>("User");

                    if (string.IsNullOrEmpty(User.HeadshotPath))
                    {
                        User.HeadshotPath = "Generic.jpg";
                    }

                    <div class="d-flex align-items-center cus-bg-primary" style="border-bottom: 2px solid var(--secondary-color); height: 4rem;">
                        <i class="fa-solid fa-bars cus-text-secondary-text me-3" data-bs-toggle="modal" data-bs-target="#divMenu" style="cursor: pointer; font-size: 1.75rem; padding: 0.5rem 0 0.5rem 0.75rem;"></i>
                        <div style="line-height: 1.25rem;">
                            <span class="cus-text-secondary-text-no-hover me-2" style="font-size: 12pt; line-height: 1rem; white-space: nowrap;">@SystemFunctions.CreateSubstring(30, User.FullName)</span>
                            <br />
                            <span class="badge cus-text-secondary-text-no-hover cus-bg-secondary me-2">@User.Role</span>
                        </div>
                        <a class="text-center ms-auto me-2 py-2" href="~/UserFiles/@User.HeadshotPath" target="_blank" style="cursor: pointer;">
                            <img class="bg-white" src="~/UserFiles/@User.HeadshotPath" onerror="this.onerror = null; this.src='/UserFiles/Generic.jpg'; this.parentElement.href='/UserFiles/Generic.jpg';" style="border-radius: 50%; width: 45px; height: 45px;">
                        </a>
                    </div>

                    <div class="modal-body d-flex flex-column p-0 cus-bg-primary">
                        @switch (User.Role)
                        {
                            case "Super Admin":
