                                    <div class="collapse border-top border-bottom @Context.Request.Cookies["SearchAFile_Menu_Maintain"]" id="divCollapseMaintain">
                                        <a href="~/MaintainClients/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Clients">
                                            <i class="fa-solid fa-people-group nav_icon" style="width: 25px;"></i> Clients
                                        </a>
                                        <a href="~/MaintainReferralSources/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Referral Sources">
                                            <i class="fa-solid fa-people-arrows nav_icon" style="width: 25px;"></i> Referral Sources
                                        </a>
                                        <a href="~/MaintainSessionTypes/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Session Types">
                                            <i class="fa-solid fa-users-rectangle nav_icon" style="width: 25px;"></i> Session Types
                                        </a>
                                        <a href="~/MaintainServiceTypes/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Services">
                                            <i class="fa-solid fa-handshake-angle nav_icon" style="width: 25px;"></i> Services
                                        </a>
                                        <a href="~/MaintainSpecialClassifications/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Special Classifications">
                                            <i class="fa-solid fa-id-badge nav_icon" style="width: 25px;"></i> Special Classifications
                                        </a>
                                        <a href="~/MaintainTherapists/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Therapists">
                                            <i class="fa-solid fa-person-chalkboard nav_icon" style="width: 25px;"></i> Therapists
                                        </a>
                                        <a href="~/MaintainVictimizationTypes/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Victimization Types">
                                            <i class="fa-solid fa-person-falling-burst nav_icon" style="width: 25px;"></i> Victimization Types
                                        </a>
                                    </div>
                                    <a class="nav_link cus-text-secondary-text d-flex cus-collapse" title="Reports" data-bs-toggle="collapse" href="#divCollapseReports" role="button" aria-expanded="false" aria-controls="divCollapseReports" onclick="ClickMenuDropdownReports();">
                                        <i class="fa-solid fa-chart-column cus-bounce-on-click" style="font-size: 20px; width: 25px; transition-duration: 0.5s; animation-iteration-count: 1 !important"></i> <span class="d-flex w-100 cus-text-secondary-text">Reports <i id="icoReports" class="fa-solid fa-angle-left ms-auto me-3 align-self-center @Context.Request.Cookies["SearchAFile_Menu_ReportsCarat"]" style="transition-duration: 0.5s;"></i></span>
                                    </a>
                                    <div class="collapse border-top border-bottom @Context.Request.Cookies["SearchAFile_Menu_Reports"]" id="divCollapseReports">
                                        <a href="~/Reports/PMTReport" class="nav_link cus-text-secondary-text cus-sub-item" title="PMT Report">
                                            <i class="fa-solid fa-chart-line nav_icon" style="width: 25px;"></i> PMT
                                        </a>
