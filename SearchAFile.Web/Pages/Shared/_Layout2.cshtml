                                            <i class="fa-solid fa-building nav_icon" style="width: 25px;"></i> Organization
                                        </a>*@
                                    </div>
                                    <a href="~/Common/AccountSettings" class="nav_link cus-text-secondary-text mt-auto" title="Account Settings">
                                        <i class="fa-solid fa-user-cog nav_icon" style="width: 25px;"></i> Account Settings
                                    </a>
                                    @if (Convert.ToBoolean(Context.Session.GetBoolean("IsSuperAdmin")))
                                    {
                                        <a href="~/Common/LogIn" class="nav_link cus-text-secondary-text" title="Log In">
                                            <i class="fa-solid fa-sign-in nav_icon" style="width: 25px;"></i> Log In
                                        </a>
                                    }
                                </div>
                                <a class="nav_link cus-text-secondary-text mt-auto" onclick="$('#logout_form').submit();" style="border-top: 2px solid var(--secondary-color); cursor: pointer;" title="Log Out">
                                    <i class="fa-solid fa-sign-out nav_icon" style="width: 25px;"></i> Log Out
