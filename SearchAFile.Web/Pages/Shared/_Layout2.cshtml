                                            <i class="fa-solid fa-building nav_icon" style="width: 25px;"></i> Organization
                                        </a>*@
                                    </div>
                                    <a href="~/MaintainUsers/List" class="nav_link cus-text-secondary-text mt-auto" title="Users">
                                        <i class="fa-solid fa-users nav_icon" style="width: 25px;"></i> Users
                                    </a>
                                    <a href="~/Admins/OrganizationSettings" class="nav_link cus-text-secondary-text" title="Organization Settings">
                                        <i class="fa-regular fa-building nav_icon" style="width: 25px;"></i> Organization Settings
                                    </a>
                                    <a href="~/Common/AccountSettings" class="nav_link cus-text-secondary-text" title="Account Settings">
                                        <i class="fa-solid fa-user-gear nav_icon" style="width: 25px;"></i> Account Settings
                                    </a>
                                    @if (Convert.ToBoolean(Context.Session.GetBoolean("IsSuperAdmin")))
                                    {
                                        <a href="~/Common/LogIn" class="nav_link cus-text-secondary-text" title="Log In">
                                            <i class="fa-solid fa-sign-in nav_icon" style="width: 25px;"></i> Log In
                                        </a>
                                    }
                                </div>
                                <a class="nav_link cus-text-secondary-text mt-auto" onclick="$('#logout_form').submit();" style="border-top: 2px solid var(--secondary-color); cursor: pointer;" title="Log Out">
                                    <i class="fa-solid fa-sign-out nav_icon" style="width: 25px;"></i> Log Out
                                </a>
                                break;

                            case "User":
                                <a href="~/Users/Dashboard" class="nav_link cus-text-secondary-text" title="Dashboard">
                                    <i class="fa-solid fa-home nav_icon" style="width: 25px;"></i> Dashboard
                                </a>
                                <div class="overflow-auto h-100 d-flex flex-column">
                                    <a class="nav_link cus-text-secondary-text d-flex cus-collapse" title="Maintain" data-bs-toggle="collapse" href="#divCollapseMaintain" role="button" aria-expanded="false" aria-controls="divCollapseMaintain" onclick="ClickMenuDropdownMaintain();">
                                        <i class="fa-solid fa-gear nav_icon" style="width: 25px; transition-duration: 0.5s;"></i> <span class="d-flex w-100 cus-text-secondary-text">Maintain <i id="icoMaintain" class="fa-solid fa-angle-left ms-auto me-3 align-self-center  @Context.Request.Cookies["SearchAFile_Menu_MaintainCarat"]" style="transition-duration: 0.5s;"></i></span>
                                    </a>
                                    <div class="collapse border-top border-bottom @Context.Request.Cookies["SearchAFile_Menu_Maintain"]" id="divCollapseMaintain">
                                        <a href="~/MaintainClients/List" class="nav_link cus-text-secondary-text cus-sub-item" title="Maintain Clients">
                                            <i class="fa-solid fa-people-group nav_icon" style="width: 25px;"></i> Clients
                                        </a>
                                    </div>
                                    <a class="nav_link cus-text-secondary-text d-flex cus-collapse" title="Reports" data-bs-toggle="collapse" href="#divCollapseReports" role="button" aria-expanded="false" aria-controls="divCollapseReports" onclick="ClickMenuDropdownReports();">
                                        <i class="fa-solid fa-chart-column cus-bounce-on-click" style="font-size: 20px; width: 25px; transition-duration: 0.5s; animation-iteration-count: 1 !important"></i> <span class="d-flex w-100 cus-text-secondary-text">Reports <i id="icoReports" class="fa-solid fa-angle-left ms-auto me-3 align-self-center @Context.Request.Cookies["SearchAFile_Menu_ReportsCarat"]" style="transition-duration: 0.5s;"></i></span>
                                    </a>
                                    <div class="collapse border-top border-bottom @Context.Request.Cookies[" SearchAFile_Menu_Reports"]" id="divCollapseReports">
                                        <a href="~/Reports/PMTReport" class="nav_link cus-text-secondary-text cus-sub-item" title="PMT Report">
                                            <i class="fa-solid fa-chart-line nav_icon" style="width: 25px;"></i> PMT
                                        </a>
