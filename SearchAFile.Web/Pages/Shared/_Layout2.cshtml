    @await RenderSectionAsync("Scripts", required: false)

    <script type="text/javascript">

        $('#body').removeClass("modal-open");

        let MenuInterval;

        function RefreshMenu() {

            // Refresh the table.
            $('#MenuLoadCompleteFlag').val('0');
            $('#divMenuContent').load(PageURL + ' #divMenuContent>*');
            MenuInterval = setInterval(CheckMenuLoadCompleteFlag, 300);
        }

        function CheckMenuLoadCompleteFlag() {

            if ($('#MenuLoadCompleteFlag').val() == '1') {

                clearInterval(MenuInterval);
                SetMenuHighlight();
            }
        }

        function ClickMenuDropdownMaintain () {

            $('.fa-gear').toggleClass('fa-rotate-90');
            $('#icoMaintain').toggleClass('fa-rotate-90');

            if (getCookie('SearchAFile_Menu_Maintain') == '') {

                setCookie('SearchAFile_Menu_MaintainCarat', 'fa-rotate-90');
                setCookie('SearchAFile_Menu_Maintain', 'show');
            }
            else {

                setCookie('SearchAFile_Menu_MaintainCarat', '');
                setCookie('SearchAFile_Menu_Maintain', '');
            }
        }

        function ClickMenuDropdownReports() {

            $('.cus-bounce-on-click').removeClass('fa-bounce');
            setTimeout(function () { $('.cus-bounce-on-click').addClass('fa-bounce'); }, 100);
            setTimeout(function () { $('.cus-bounce-on-click').removeClass('fa-bounce'); }, 1100);
            $('#icoReports').toggleClass('fa-rotate-90');

            if (getCookie('SearchAFile_Menu_Reports') == '') {

                setCookie('SearchAFile_Menu_ReportsCarat', 'fa-rotate-90');
                setCookie('SearchAFile_Menu_Reports', 'show');
            }
            else {

